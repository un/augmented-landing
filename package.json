{
  "name": "augment",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "dotenv -e .env.local -- turbo dev --env-mode=loose",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:landing": "turbo dev:landing",
    "build:landing": "turbo build:landing",
    "start:landing": "turbo start:landing",
    "lint:landing": "turbo lint:landing"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "turbo": "^2.4.0",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "dotenv-cli": "^8.0.0"
  }
}